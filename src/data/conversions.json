        {
            "name": "Celsius to Fahrenheit",
            "formula": "F = \\frac{9}{5}C + 32",
            "variables": {
                "F": "Temperature in Fahrenheit (°F)",
                "C": "Temperature in Celsius (°C)"
            },
            "description": "Converts temperature from Celsius to Fahrenheit.",
            "example": {
                "problem": "\\text{Convert } 25 \\, ^\\circ\\text{C} \\text{ to Fahrenheit.}",
                "solution": "\\begin{aligned}\nF &= \\frac{9}{5}C + 32 \\\\\n&= \\frac{9}{5} \\cdot 25 + 32 \\\\\n&= 45 + 32 \\\\\n&= \\boxed{77 \\, ^\\circ\\text{F}}\n\\end{aligned}"
            }
        },
        {
            "name": "Kelvin to Celsius",
            "formula": "C = K - 273.15",
            "variables": {
                "C": "Temperature in Celsius (°C)",
                "K": "Temperature in Kelvin (K)"
            },
            "description": "Converts temperature from Kelvin to Celsius.",
            "example": {
                "problem": "\\text{Convert } 300 \\, \\text{K} \\text{ to Celsius.}",
                "solution": "\\begin{aligned}\nC &= K - 273.15 \\\\\n&= 300 - 273.15 \\\\\n&= \\boxed{26.85 \\, ^\\circ\\text{C}}\n\\end{aligned}"
            }
        },
        {
            "name": "Fahrenheit to Kelvin",
            "formula": "K = \\frac{5}{9}(F - 32) + 273.15",
            "variables": {
                "K": "Temperature in Kelvin (K)",
                "F": "Temperature in Fahrenheit (°F)"
            },
            "description": "Converts temperature from Fahrenheit to Kelvin.",
            "example": {
                "problem": "\\text{Convert } 68 \\, ^\\circ\\text{F} \\text{ to Kelvin.}",
                "solution": "\\begin{aligned}\nK &= \\frac{5}{9}(F - 32) + 273.15 \\\\\n&= \\frac{5}{9}(68 - 32) + 273.15 \\\\\n&= \\frac{5}{9} \\cdot 36 + 273.15 \\\\\n&= 20 + 273.15 \\\\\n&= \\boxed{293.15 \\, \\text{K}}\n\\end{aligned}"
            }
        },
